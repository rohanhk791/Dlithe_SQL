alter database employee modify name=Dlithe_sql
use Dlithe_sql

drop table emp1
create table emp1
(
Id int,
Name varchar(20),
Designation varchar(10),
)
insert into emp1 values(1,'prajwal','SE')
insert into emp1 values(2,'mahesh','SE')
insert into emp1 values(3,'deepak','HR')


